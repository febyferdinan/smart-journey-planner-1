# AviationStack API Key (https://aviationstack.com/)
AVIATIONSTACK_API_KEY=your_aviationstack_api_key_here

# Mapbox API Token (required for Mapbox provider)
# Get yours at https://account.mapbox.com/access-tokens/
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here

# Google Maps API Key (required for Google Maps provider)
# Get yours at https://console.cloud.google.com/apis/credentials
# Enable: Geocoding API, Directions API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_openai_api_key_here

# AviationStack API Key (required for flight tracking)
# Only configure the providers you plan to use
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_google_gemini_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here

